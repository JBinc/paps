:@template"Í3/*Color Variables*/
$primary: #1a4e87;
$secondary: #9ca09f;
$font: #575757;
$darkFont: #404040;
$fontSize: 16px;
$gridWidth: 960px;


body{
    background-color: #ffffff;
    margin: 0;
    color: $font;
}

a{
    color: $primary;
}

h1,h2,h3,h4,h5{
    font-weight: normal;
}


/*Page Banner*/

.entry-content h2, .comment-content h2, .mu_register h2 {
    background-color: #1A4E87;
    color: #FFFFFF;
    line-height: 1.6;
    margin-bottom: 20px;
    padding-left: 20px;
    text-shadow: 2px 2px 3px #292929;
}

/*Grid Settings*/
.container{
    width: 60em;
    margin: 0 auto;
}

#secondary{
    float: right;
    margin-top: 0px;
}

/*Header Settings*/
.site-header{
    padding: 0;
}

/*Main Menu Navigation*/
#site-navigation{
    background-image: url('img/nav-bg.png');
    background-repeat: repeat-x;
}

#nav-search{
    float: left;
    margin-right: percentage(15px/$gridWidth);
    input{
        border: 1px solid #e3e3e3;
        color: #797979;
        -webkit-border-radius: 5px;
        -moz-border-radius: 5px;
        border-radius: 5px;
    }
}

#site-navigation{
    padding-top: 10px;
    height: 60px;
    ul{
        margin: 0;
        padding: 7px 0;
    }
    li{
        display: inline-block;
        border-right: 2px solid #dedfdf;
        margin-right: 1.8%;
        padding-right: 1.8%;
        a{
            color: $font;
            text-decoration: none;
            text-shadow: 1px 0 1px #c9c9c9;
        }
        a:hover{
            color: $primary;
        }
    }
    li:last-child{
        border-right: none;
    }
}

#logo{
    img{
        margin: 10px 0;
    }
}

/*Home Page*/
.home{
    #primary{
        margin-top: 0!important;
    }
}

#panel-0-1-0{
    height: 364px;
}

#pgc-2-1{
     ul{
        border-left: none;
        padding: 0;
    }
}

/*Call to Action Form*/
#pg-0{
    margin-bottom: 0!important;
    overflow: hidden;
    .panel-grid-cell{
        padding: 0;
    }

    .panel-grid-cell{
        margin-bottom: 0!important;
    }
}

#pgc-0-0{
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(left,  rgba(255,255,255,0) 60%, rgba(216,216,216,0) 71%, rgba(135,135,135,1) 94%, rgba(114,114,114,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(60%,rgba(255,255,255,0)), color-stop(71%,rgba(216,216,216,0)), color-stop(94%,rgba(135,135,135,1)), color-stop(100%,rgba(114,114,114,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left,  rgba(255,255,255,0) 60%,rgba(216,216,216,0) 71%,rgba(135,135,135,1) 94%,rgba(114,114,114,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left,  rgba(255,255,255,0) 60%,rgba(216,216,216,0) 71%,rgba(135,135,135,1) 94%,rgba(114,114,114,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left,  rgba(255,255,255,0) 60%,rgba(216,216,216,0) 71%,rgba(135,135,135,1) 94%,rgba(114,114,114,1) 100%); /* IE10+ */
    background: linear-gradient(to right,  rgba(255,255,255,0) 60%,rgba(216,216,216,0) 71%,rgba(135,135,135,1) 94%,rgba(114,114,114,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#727272',GradientType=1 ); /* IE6-8 */

}

#cta-form-hold{
    background: #fdb800; /* Old browsers */


    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(-45deg,  #fdb800 0%, #fdb800 49%, #fed466 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right bottom, color-stop(0%,#fdb800), color-stop(49%,#fdb800), color-stop(100%,#fed466)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(-45deg,  #fdb800 0%,#fdb800 49%,#fed466 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(-45deg,  #fdb800 0%,#fdb800 49%,#fed466 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(-45deg,  #fdb800 0%,#fdb800 49%,#fed466 100%); /* IE10+ */
    background: linear-gradient(135deg,  #fdb800 0%,#fdb800 49%,#fed466 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#fdb800', endColorstr='#fed466',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */

    -webkit-border-top-left-radius: 25px;
    -webkit-border-bottom-left-radius: 25px;
    -moz-border-radius-topleft: 25px;
    -moz-border-radius-bottomleft: 25px;
    border-top-left-radius: 25px;
    border-bottom-left-radius: 25px;


    height: 365px;

    #si_contact_form1{
        width: 80%;
        margin: 0 auto;

        label{
            color: #ffffff;
            text-shadow: 2px 0 2px #ab8214;
            font-size: 100%;
        }

        input{
            width: 95%;
            border: 1px solid #bca054;
            -webkit-border-radius: 5px;
            -moz-border-radius: 5px;
            border-radius: 5px;
            padding: 5px;
        }

        input[type="submit"]{
            margin-top: 20px;
            color: $darkFont;
            padding: 3%;
            background-color: #fdc224;
            border: 1px solid #d6a729;
            text-shadow: 2px 0 4px #fff0c9;
            -webkit-border-radius: 10px;
            -moz-border-radius: 10px;
            border-radius: 10px;
            -webkit-box-shadow: inset 0px 0px 5px 1px rgba(254, 220, 128, 1.0);
            box-shadow: inset 0px 0px 5px 1px rgba(254, 220, 128, 1.0);
            width: 70%;
            font-size: 1.2em;
        }
        input[type="submit"]:hover{
            -webkit-box-shadow: inset 0px 0px 50px 1px rgba(254, 220, 128, 1.0);
            box-shadow: inset 0px 0px 50px 1px rgba(254, 220, 128, 1.0);
        }
    }
    h2{
        text-align: center;
        margin: 0;
        color: $darkFont;
        font-size: 1.75em;
        text-shadow: 2px 0 4px #fff0c9;
        padding-top: 10px;
        background: none;
    }
}

/*Home - 5 Column*/
#pg-1{
    margin-top: 15px;
    .panel-grid-cell{
        width: 19.8%;
        margin-right: .2%;
        padding: 0!important;
    }
    .panel{
        padding: 5.225%;
        min-height: 360px;
        position: relative;
        color: #ffffff;

        h3{
            font-weight: normal;
            padding-left: percentage(5px/198px);
            font-size: 110.75%;
        }

        h4{
            font-size: percentage(18px/$fontSize);
            margin-top: 10px;
        }

        p{
            font-size: percentage(14px/$fontSize);
            margin-top: 15px;
        }

    }
    .read-more{
        color: #ffffff;
        text-decoration: none;
        position: absolute;
        right: percentage(30px / 198px);
        bottom: 5px;
        z-index: 9999;
    }
    .read-more:hover{
        text-decoration: underline;
    }
    .opac-bg{
        position: absolute;
        bottom: 0;
        left: 0;
        background-color: #000000;
        -moz-opacity: 0.50;
        opacity: 0.50;
        display: block;
        width: 100%;
        height: percentage(34px / 360px);

        -webkit-border-bottom-right-radius: 25px;
        -webkit-border-bottom-left-radius: 25px;
        -moz-border-radius-bottomright: 25px;
        -moz-border-radius-bottomleft: 25px;
        border-bottom-right-radius: 25px;
        border-bottom-left-radius: 25px;
    }

    #pgc-1-0{background-color: #eeeeee;}
    #pgc-1-1{background-color: #828282;}
    #pgc-1-2{background-color: #159595;}
    #pgc-1-3{background-color: #95342b;}
    #pgc-1-4{background-color: #4d4d4d;}

    #panel-1-0-0{color: #6f6f6f;}

}

#pg-2{
    margin: 15px 0;
    .widget-title, .widget-title a{
        color: $font;
        font-weight: normal;
        text-decoration: none;
        font-size: percentage(24px/$fontSize)
    }

    #panel-2-1-0 h3 a{
    font-weight: normal;
    padding-left: percentage(5px/198px);
    font-size: 125%;
}

    .panel{
        margin-right: 4%;
    }
}

.testimonials:before{
    content:' " ';
    font-size: percentage(43px/$fontSize);
    position: absolute;
    top: -35px;
    left: -6%;
    color: #bfbfbf;
}

.testimonials{
    position: relative;
    color: #1a4f87;
    font-size: 112.5%;
    margin-top: 20px;
    margin-left: percentage(15px/301px);

}

/*Rss Feed*/
.widget_rss{
    font-size: percentage(14px/$fontSize);
    .widget-title{
        font-size: 100%;
    }
    .widget-title a:first-child{
        display: none;
    }

    a{
        color: $font;
        text-decoration: none;
    }

    ul{
        list-style-type: none;
        li{
            margin: 15px 0;
        }

        li:before{
            content: '';
            background-image: url('img/rss-item.png');
            background-repeat: none;
            width: 18px;
            height: 18px;
            float: left;
            margin-right: percentage(5px / 330px);
        }
    }

    .rss-date{
        display: block;
        font-size: percentage(12px/$fontSize);
        color: $primary;
    }
    .rssSummary{
        padding-top: 10px;
    }
}

.widget-area .widget h3{
    font-size: percentage(24px/$fontSize);
    font-weight: normal;
    margin-left: percentage(20px/$gridWidth);
}

.widget_rss ul{
    border-left: 2px solid #EEEEEE;
    padding-left: 7%;
}

/*Footer Settings*/
#footer{
    background-color: #051f3b;
    background: #051f3b; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(-45deg,  #051f3b 0%, #09386a 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right bottom, color-stop(0%,#051f3b), color-stop(100%,#09386a)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(-45deg,  #051f3b 0%,#09386a 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(-45deg,  #051f3b 0%,#09386a 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(-45deg,  #051f3b 0%,#09386a 100%); /* IE10+ */
    background: linear-gradient(135deg,  #051f3b 0%,#09386a 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#051f3b', endColorstr='#09386a',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
    height: 215px;
    padding: 20px 0;
}

#footer-sidebar{
    border-bottom: 1px solid #1f374f;
    padding-bottom: 15px;
    height: 150px;
    h3{
        font-size: percentage(24px/$fontSize);
        color: #ffffff;
        font-weight: normal;
    }
    a{
        display: block;
        color: #ffffff;
        text-decoration: none;
        font-size: percentage(13px/$fontSize);
    }
    a:hover{
        text-decoration: underline;
    }

    .footer-item{
        border-right: 1px solid #1f374f;
        width: 24%;
        padding: 0 5%;
    }

    .footer-item:last-child{
        padding-right: 0;
        border-right: none;
    }
}

.footer-item{
    width: 33%;
    float: left;
}

.copyright{
    color: #ffffff;
    text-align: right;
    padding-right: percentage(40px / 960px);
    padding-top: 15px;
    font-size: percentage(12px/$fontSize);
